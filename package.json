{
  "name": "blockchange-nuxthub",
  "version": "0.1.0",
  "scripts": {
    "dev": "node scripts/generate-sitemap.mjs && NUXT_HUB_REMOTE=true nuxi dev --remote",
    "prebuild": "node scripts/generate-sitemap.mjs",
    "build": "nuxi build",
    "start": "nuxi start",
    "sitemap": "node scripts/generate-sitemap.mjs",
    "generate": "NUXT_SSG=true node scripts/generate-sitemap.mjs && NUXT_SSG=true nuxi generate && node scripts/post-generate.mjs",
    "preview:ssg": "npx serve .output/public -l 3000"
  },
  "author": "Learntheropes <giovanni@learntheropes.xyz",
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@nuxt/content": "3.5.1",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "^9.5.4",
    "@oruga-ui/oruga-next": "0.6.0",
    "@oruga-ui/theme-bulma": "0.2.11",
    "lodash.find": "4.6.0",
    "nuxt": "3.17.3",
    "nuxt-calcom": "^1.1.9",
    "nuxt-delay-hydration": "1.3.8",
    "nuxt-umami": "3.2.0",
    "sass": "1.63.6",
    "sass-loader": "13.3.2"
  },
  "devDependencies": {
    "nuxthub": "^0.9.0",
    "wrangler": "^4.42.2"
  }
}